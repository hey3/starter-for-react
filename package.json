{
  "name": "starter-for-react",
  "version": "1.0.0",
  "description": "hey3's boilerplate for Web Frontend",
  "keywords": [
    "react",
    "typescript"
  ],
  "homepage": "https://github.com/hey3/starter-for-react#readme",
  "bugs": {
    "url": "https://github.com/hey3/starter-for-react/issues"
  },
  "license": "MIT",
  "author": "hey3",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hey3/starter-for-react.git"
  },
  "scripts": {
    "build-storybook": "build-storybook",
    "build:dev": "BUILD_MODE='development' REACT_APP_ENV='development' webpack",
    "build:local": "BUILD_MODE='development' REACT_APP_ENV='local' webpack",
    "build:prod": "BUILD_MODE='production' REACT_APP_ENV='production' webpack",
    "build:stg": "BUILD_MODE='production' REACT_APP_ENV='staging' webpack",
    "fix": "fixpack",
    "format": "prettier --write './src/**/*.{ts,tsx,json,js}'",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint 'src/**/*.ts{,x}'",
    "lint:style": "stylelint 'src/**/*.ts{,x}'",
    "start:dev": "BUILD_MODE='development' REACT_APP_ENV='development' webpack serve",
    "start:local": "BUILD_MODE='development' REACT_APP_ENV='local' webpack serve",
    "start:prod": "BUILD_MODE='production' REACT_APP_ENV='production' webpack serve",
    "start:stg": "BUILD_MODE='production' REACT_APP_ENV='staging' webpack serve",
    "storybook": "start-storybook -p 6006",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --passWithNoTests --coverage",
    "test:virtual-update": "jest --updateSnapshot",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "core-js": "^3.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@storybook/theming": "^6.2.9",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "babel-loader": "^8.2.2",
    "dotenv-webpack": "^7.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "fixpack": "^4.0.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "react-test-renderer": "^17.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.3.4",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
