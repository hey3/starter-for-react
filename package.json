{
  "name": "starter-for-react",
  "version": "1.0.0",
  "description": "hey3's boilerplate for Web Frontend",
  "homepage": "https://github.com/hey3/starter-for-react#readme",
  "bugs": {
    "url": "https://github.com/hey3/starter-for-react/issues"
  },
  "license": "MIT",
  "author": "hey3",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hey3/starter-for-react.git"
  },
  "scripts": {
    "build-storybook": "build-storybook",
    "build:dev": "BUILD_MODE='development' REACT_APP_ENV=development webpack",
    "build:local": "BUILD_MODE='development' REACT_APP_ENV=local webpack",
    "build:prd": "BUILD_MODE='production' REACT_APP_ENV=production webpack",
    "build:stg": "BUILD_MODE='production' REACT_APP_ENV=staging webpack",
    "fix": "fixpack",
    "format": "prettier --write './src/**/*.{ts,tsx,json,js}'",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint 'src/**/*.ts{,x}'",
    "lint:style": "stylelint 'src/**/*.ts{,x}'",
    "lint:tsc": "tsc --noEmit",
    "start:dev": "BUILD_MODE='development' REACT_APP_ENV=development webpack-dev-server --open",
    "start:local": "BUILD_MODE='development' REACT_APP_ENV=local webpack-dev-server --open",
    "start:prd": "BUILD_MODE='production' REACT_APP_ENV='production' webpack-dev-server --open",
    "start:stg": "BUILD_MODE='production' REACT_APP_ENV='staging' webpack-dev-server --open",
    "storybook": "start-storybook -p 6006",
    "test": "jest"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@hey3/eslint-config": "^1.0.0",
    "@storybook/addon-a11y": "^5.3.17",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-viewport": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/cli": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@storybook/theming": "^5.3.17",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.4",
    "@types/styled-components": "^5.0.1",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "file-loader": "^6.0.0",
    "fixpack": "^3.0.6",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.3",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "storybook-readme": "^5.0.8",
    "styled-reset": "^4.1.2",
    "stylelint": "^13.3.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix"
    ],
    "*.{ts,tsx,js,json}": [
      "prettier --write"
    ],
    "*package.json": [
      "fixpack"
    ]
  }
}
