{
  "name": "starter-for-react",
  "version": "1.0.0",
  "description": "hey3's boilerplate for Web Frontend",
  "keywords": [
    "react",
    "typescript"
  ],
  "homepage": "https://github.com/hey3/starter-for-react#readme",
  "bugs": {
    "url": "https://github.com/hey3/starter-for-react/issues"
  },
  "license": "MIT",
  "author": "hey3",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hey3/starter-for-react.git"
  },
  "scripts": {
    "build-storybook": "build-storybook",
    "build:dev": "BUILD_MODE='development' REACT_APP_ENV='development' webpack",
    "build:local": "BUILD_MODE='development' REACT_APP_ENV='local' webpack",
    "build:prod": "BUILD_MODE='production' REACT_APP_ENV='production' webpack",
    "build:stg": "BUILD_MODE='production' REACT_APP_ENV='staging' webpack",
    "fix": "fixpack",
    "format": "prettier --write './src/**/*.{ts,tsx,json,js}'",
    "lint": "yarn lint:eslint && yarn lint:style",
    "lint:eslint": "eslint 'src/**/*.ts{,x}'",
    "lint:style": "stylelint 'src/**/*.ts{,x}'",
    "start:dev": "BUILD_MODE='development' REACT_APP_ENV='development' webpack serve",
    "start:local": "BUILD_MODE='development' REACT_APP_ENV='local' webpack serve",
    "start:prod": "BUILD_MODE='production' REACT_APP_ENV='production' webpack serve",
    "start:stg": "BUILD_MODE='production' REACT_APP_ENV='staging' webpack serve",
    "storybook": "start-storybook -p 6006",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --passWithNoTests --coverage",
    "test:virtual-update": "jest --updateSnapshot",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "core-js": "3.20.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.2.1",
    "styled-components": "5.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.16.12",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@stylelint/postcss-css-in-js": "0.37.2",
    "@storybook/addon-a11y": "6.4.19",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addons": "6.4.19",
    "@storybook/builder-webpack5": "6.4.19",
    "@storybook/manager-webpack5": "6.4.19",
    "@storybook/react": "6.4.19",
    "@storybook/testing-react": "1.2.3",
    "@storybook/theming": "6.4.19",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.0",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/styled-components": "5.1.22",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "babel-loader": "8.2.3",
    "dotenv-webpack": "7.1.0",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "fixpack": "4.0.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "27.4.7",
    "jest-styled-components": "7.0.8",
    "postcss-syntax": "0.36.2",
    "prettier": "2.5.1",
    "prettier-plugin-organize-imports": "2.3.4",
    "stylelint": "14.5.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "24.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5",
    "webpack": "5.69.1",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4"
  }
}
