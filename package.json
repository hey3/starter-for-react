{
  "name": "starter-for-react",
  "version": "1.0.0",
  "description": "hey3's boilerplate for Web Frontend",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "pnpm",
    "boilerplate-template"
  ],
  "homepage": "https://github.com/hey3/starter-for-react#readme",
  "bugs": {
    "url": "https://github.com/hey3/starter-for-react/issues"
  },
  "license": "MIT",
  "author": "hey3",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hey3/starter-for-react.git"
  },
  "packageManager": "pnpm@7.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier './src/**/*.{ts,tsx,json,js}'",
    "format:fix": "prettier --write './src/**/*.{ts,tsx,json,js}'",
    "lint": "pnpm lint:eslint && pnpm lint:style",
    "lint:fix": "pnpm lint:eslint-fix && pnpm lint:style-fix",
    "lint:eslint": "eslint 'src/**/*.ts{,x}'",
    "lint:eslint-fix": "eslint --fix 'src/**/*.ts{,x}'",
    "lint:style": "stylelint 'src/**/*.ts{,x}'",
    "lint:style-fix": "stylelint --fix 'src/**/*.ts{,x}'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --passWithNoTests --coverage",
    "test:virtual-update": "vitest run -u",
    "typecheck": "tsc --project tsconfig.json --pretty --noEmit --incremental false"
  },
  "dependencies": {
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-router-dom": "6.3.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "0.37.3",
    "@storybook/addon-a11y": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/builder-vite": "0.1.33",
    "@storybook/react": "6.4.22",
    "@storybook/testing-react": "1.2.4",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@testing-library/user-event": "14.1.1",
    "@types/node": "16.11.33",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "@vitejs/plugin-react": "1.3.2",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "jsdom": "19.0.0",
    "postcss-syntax": "0.36.2",
    "prettier": "2.6.2",
    "stylelint": "14.7.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-standard": "25.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "typescript": "4.6.4",
    "vite": "2.9.8",
    "vitest": "0.9.4"
  },
  "dependenciesComments": {
    "@types/node": [
      "Using Node LTS version"
    ],
    "@storybook/addon-a11y": [
      "Not yet available in storybook-builder-vite"
    ]
  }
}
