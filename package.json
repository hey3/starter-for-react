{
  "name": "starter-for-react",
  "version": "1.0.0",
  "description": "hey3's boilerplate for Web Frontend",
  "homepage": "https://github.com/hey3/starter-for-react#readme",
  "bugs": {
    "url": "https://github.com/hey3/starter-for-react/issues"
  },
  "license": "MIT",
  "author": "hey3",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hey3/starter-for-react.git"
  },
  "scripts": {
    "build-storybook": "build-storybook",
    "build:dev": "BUILD_MODE='development' REACT_APP_ENV='development' webpack",
    "build:local": "BUILD_MODE='development' REACT_APP_ENV='local' webpack",
    "build:prod": "BUILD_MODE='production' REACT_APP_ENV='production' webpack",
    "build:stg": "BUILD_MODE='production' REACT_APP_ENV='staging' webpack",
    "fix": "fixpack",
    "format": "prettier --write './src/**/*.{ts,tsx,json,js}'",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint 'src/**/*.ts{,x}'",
    "lint:style": "stylelint 'src/**/*.ts{,x}'",
    "lint:tsc": "tsc --noEmit",
    "start:dev": "BUILD_MODE='development' REACT_APP_ENV='development' webpack-dev-server --open",
    "start:local": "BUILD_MODE='development' REACT_APP_ENV='local' webpack-dev-server --open",
    "start:prod": "BUILD_MODE='production' REACT_APP_ENV='production' webpack-dev-server --open",
    "start:stg": "BUILD_MODE='production' REACT_APP_ENV='staging' webpack-dev-server --open",
    "storybook": "start-storybook -p 6006",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --passWithNoTests --coverage",
    "test:virtual-update": "jest --updateSnapshot"
  },
  "dependencies": {
    "core-js": "^3.8.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@storybook/addon-a11y": "^6.1.10",
    "@storybook/addon-essentials": "^6.1.10",
    "@storybook/addon-knobs": "^6.1.10",
    "@storybook/addons": "^6.1.10",
    "@storybook/react": "^6.1.10",
    "@storybook/theming": "^6.1.10",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.16",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-test-renderer": "^17.0.0",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.3.2",
    "babel-loader": "^8.2.2",
    "dotenv-webpack": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "fixpack": "^3.0.6",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix"
    ],
    "*.{ts,tsx,js,json}": [
      "prettier --write"
    ],
    "*package.json": [
      "fixpack"
    ]
  }
}
